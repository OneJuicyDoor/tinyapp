  <body>
    <%- include('partials/_header') %>
    <form class="form-inline ml-auto" method="POST" action="/register">
      <div class="form-group mb-2">
        <input
          class="form-control"
          type="text"
          name="email"
          placeholder="email"
          style="width: 300px; margin-right: 1em"
        />
        <input
          class="form-control"
          type="text"
          name="password"
          placeholder="password"
          style="width: 300px; margin-right: 1em"
